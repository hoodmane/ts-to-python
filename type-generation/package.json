{
  "name": "@pyodide/ts-to-python",
  "author": "Hood Chatham",
  "version": "0.1.5",
  "description": "Converting TypeScript types to Python types to use with Pyodide",
  "license": "ISC",
  "keywords": [],
  "files": [
    "dist/*"
  ],
  "type": "module",
  "bin": "dist/main.js",
  "scripts": {
    "test": "node --test --import=tsx 'tests/**/*.test.ts'",
    "run": "tsx src/main.ts",
    "build": "npx tsc && cp src/prelude.pyi dist",
    "check": "npx tsc -p tsconfig-check.json",
    "prepack": "cp ../LICENSE ../README.md . && npm run build",
    "postpack": "rm -f LICENSE README.md"
  },
  "dependencies": {
    "ts-morph": "^27.0.0",
    "tslog": "^4.10.2",
    "tsx": "^4.6.2"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "typescript": "^5.9.2"
  }
}
